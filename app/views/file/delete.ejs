<% layout('../_layout'); %>
<div class="ui-block">
  <div class="ui-block-title"><h5>Suppression de "<%-file.title%>"</h5></div>
  <div class="ui-block-content">
    <p>Voulez vous vraiment supprimer le fichier "<%-file.title%>" ?</p>
    <form action="/file/destroy" method="post" class="form">
      <input type="hidden" name="_csrf" value="<%- csrfToken %>">
      <input type="hidden" name="id" value="<%-file.id%>">
      <input type="submit" class="btn btn-default" value="Oui le supprimer">
      <div class="clear"></div>
    </form>
  </div>
</div>
