<% layout('../_layout'); %>
<div class="ui-block">
  <div class="ui-block-title"><h5>Suppression de "<%-offerstore.name%>"</h5></div>
  <div class="ui-block-content">
    <p>Voulez vous vraiment supprimer la boutique "<%-offerstore.name%>" ?</p>
    <form action="/offerstore/destroy" method="post" class="form">
      <input type="hidden" name="_csrf" value="<%=csrfToken%>">
      <input type="hidden" name="id" value="<%=offerstore.id%>">
      <input type="submit" class="btn btn-default" value="Oui la supprimer">
      <div class="clear"></div>
    </form>
  </div>
</div>
